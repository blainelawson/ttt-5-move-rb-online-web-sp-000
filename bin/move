#!/usr/bin/env ruby
require "pry"
require_relative '../lib/move.rb'

# Code your CLI Here
board = [" ", " ", " "," ", " ", " "," ", " ", " "]

puts "Welcome to Tic Tac Toe!"

puts "Where would you like to go?"

input = gets.strip
binding.pry
input_to_index(input)
binding.pry
move(board, input, "X")

display_board(board)
